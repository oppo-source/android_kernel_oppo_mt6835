		#thermal-sensor-cells = <0>;
		reg = <0x6e>;
		status = "okay";
	};
};

&i2c3 {
	status = "okay";
	clock-frequency = <400000>;
	mediatek,use-push-pull;

	mtk_flashlights_23281:flashlights_23281@64 {
		compatible = "mediatek,strobe_main";
		reg = <0x64>;
		status = "okay";
	};
};

&flashlights_23281 {
	status = "okay";
	flash@0{
		reg = <0>;
		type = <0>;
		ct = <0>;
		part = <0>;
		port@0 {
			fl_core_0: endpoint {
				remote-endpoint = <&flashlight_0>;
			};
		};
	};
};

